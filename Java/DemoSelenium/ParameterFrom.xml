<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ParameterFromXML">
    <test name ="Test1">
        <classes>

            <class name="TestNGPriotrize.Parameter">
                <parameter name="browser" value="chrome"></parameter>
                <parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"></parameter>
                <methods>
                    <include name="homeLogo"/>
                    <include name="homeTitle"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name ="Test2">
        <classes>

            <class name="TestNGPriotrize.Parameter">
                <parameter name="browser" value="firefox"></parameter>
                <parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"></parameter>
                <methods>
                    <include name="homeLogo"/>
                    <include name="homeTitle"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>